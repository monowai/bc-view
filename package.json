{
  "name": "bc-next",
  "private": true,
  "license": "GPL-3.0-or-later",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "dev": "next dev --turbopack",
    "node-debug": "NODE_OPTIONS='--inspect' next dev",
    "prettier": "prettier --write '**/*.{ts*,json,md,yml,js}' && yarn lint",
    "build": "next build",
    "start": "next start",
    "test:watch": "jest --watch",
    "test": "jest --ci --coverage --reporters=default --reporters=jest-junit",
    "lint": "next lint --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .next",
    "deps:check": "yarn outdated",
    "deps:update": "yarn upgrade-interactive",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^3.6.0",
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@hookstate/core": "^4.0.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/core": "^1.9.0",
    "@opentelemetry/instrumentation": "^0.56.0",
    "@opentelemetry/sdk-trace-base": "^1.30.1",
    "@sentry/nextjs": "^8.53",
    "@serverless-jwt/next": "^0.2.1",
    "classnames": "^2.3.2",
    "critters": "^0.0.25",
    "http-status-codes": "^2.2.0",
    "i18next": "^24.0.0",
    "i18next-fs-backend": "^2.0.0",
    "kafkajs": "^2.0.0",
    "next": "^15.0.0",
    "next-i18next": "^15.0.0",
    "papaparse": "^5.5.2",
    "ramda": "^0.30",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-dropzone": "^14.0.0",
    "react-hook-form": "^7.43.0",
    "react-i18next": "^15.4",
    "react-number-format": "^5.0.0",
    "react-select": "^5.0.0",
    "react-switch": "^7.1.0",
    "redux": "^5.0.1",
    "sharp": "^0.33.0",
    "simple-git": "^3.25.0",
    "swr": "^2.0.0",
    "yup": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@hookform/resolvers": "^3.0.0",
    "@hookstate/devtools": "^4.0.1",
    "@next/eslint-plugin-next": "^15.0.0",
    "@redux-devtools/extension": "^3",
    "@tailwindcss/postcss": "^4.0.0",
    "@testing-library/dom": "^10.1",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^22.0.0",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@typescript-eslint/typescript-estree": "^8.0.0",
    "autoprefixer": "^10.4.20",
    "babel-jest": "^29.0.0",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.0.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-jest": "^28.11",
    "eslint-plugin-jest-dom": "^5.4",
    "eslint-plugin-prettier": "^5.2",
    "eslint-plugin-testing-library": "^6.2",
    "husky": "^9.0.11",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "jest-fetch-mock": "^3.0.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.1",
    "prettier": "^3.0.0",
    "sass": "^1.83.4",
    "tailwindcss": "^4.0.6",
    "ts-jest": "^29.1.2",
    "tsconfig-paths-jest": "^0.0.1",
    "typescript": "^5.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,md}": [
      "prettier --write"
    ]
  },
  "jest-junit": {
    "output": "reports/junit/junit.xml",
    "usePathForSuiteName": "true"
  },
  "resolutions": {
    "@types/node": "^22.0.0",
    "@types/papaparse": "^5.3.15"
  }
}
